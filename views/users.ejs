<%- include partials/header.ejs %>
<h1>Users</h1>
<table>
    <thead>
        <tr>
            <th>Username</th>
            <th>User ID</th>
        </tr>
    </thead>
    <tbody>
    <% for (var i = 0; i < users.length; i++) { %>
    <tr>
        <td><%= users[i]._id %></td>
        <td><%= users[i].username %></td>
    </tr>
    <% } %>
    </tbody>
</table>
</body>
</html>